{
	"containerDefinitions": [{
		"essential": true,
		"name": "tomcat",
		"image": "466749146115.dkr.ecr.ap-south-1.amazonaws.com/tomcat:v1",
		"memory": 2048,
		"cpu": 1024,
		"portMappings": [{
			"hostPort": 80,
			"containerPort": 8080,
			"protocol": "tcp"
